{
  "expo": {
    "name": "TrapMaison",
    "slug": "TrapMaison",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/palmlogo.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#282c34"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.musta.trapmusic"
    },
    "web": {
      "favicon": "./assets/palmlogo.png"
    }
  }
}
